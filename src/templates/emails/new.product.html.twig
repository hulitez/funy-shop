{% extends 'base.html.twig' %}

{% block title %}There is new one{% endblock %}

{% block body %}
    <h1>Product</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ product.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ product.name }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ product.description }}</td>
        </tr>
        <tr>
            <th>Price</th>
            <td>{{ product.price }}</td>
        </tr>
        <tr>
            <th>Currency</th>
            <td>{{ product.currency }}</td>
        </tr>
        </tbody>
    </table>
{% endblock %}
